{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "__MSG_extensionName__",
  "author": "Gabageba",
  "content_scripts": [
    {
      "matches": ["https://*.animego.org/*"],
      "js": ["extensions/jquery-3.6.3.js", "scripts/animego.js"]
    },
    {
      "matches": ["https://*.jut.su/*"],
      "js": ["extensions/jquery-3.6.3.js", "scripts/jutsu.js"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "image/auto-skipper-icon-16x16.png",
      "32": "image/auto-skipper-icon-32x32.png",
      "48": "image/auto-skipper-icon-48x48.png",
      "128": "image/auto-skipper-icon-128x128.png"
    },
    "default_area": "navbar",
    "default_popup": "index.html",
    "default_title": "__MSG_extensionName__"
  },
  "default_locale": "en",
  "description": "__MSG_extensionDescription__",
  "icons": {
    "16": "image/auto-skipper-icon-16x16.png",
    "32": "image/auto-skipper-icon-32x32.png",
    "48": "image/auto-skipper-icon-48x48.png",
    "128": "image/auto-skipper-icon-128x128.png"
  },
  "homepage_url": "https://github.com/Gabageba",
  "permissions": ["storage"]
}
